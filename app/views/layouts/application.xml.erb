  <ModulePrefs title="Soundboardify"
               description="A collection of fun sound clips"
               author="Spencer Davis, Jason Vanderslice">
          <Require feature="rpc" />
          <Require feature="views" />\
          <Require feature="locked-domain" /> \
  </ModulePrefs>
  <Content type="html">
    <![CDATA[

    <html>
      <head>
        <title>Soundboardify</title>
        <meta http-equiv="Content-Type" content="text/html; charset = iso - 8859 - 1" />
      </head>
      <body>
        <%= yield %>
        <%= javascript_include_tag "https://plus.google.com/hangouts/_/api/v1/hangout.js" %>
        <%= javascript_include_tag 'application' %>
      </body>
    </html>

    ]]>
  </Content>
